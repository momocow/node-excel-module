{
  "name": "excel-module",
  "version": "1.0.1",
  "description": "Expose excel functions in a XLSX file as a JavaScript module.",
  "main": "dist/index.js",
  "scripts": {
    "commit": "gitmoji -c",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "test:quick": "test -d ./dist && ava ./test/**/*.test.js",
    "test": "npm run build && ava ./test/**/*.test.js",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momocow/node-excel-module.git"
  },
  "keywords": [
    "excel",
    "xlsx",
    "spreadsheet",
    "sheet",
    "module",
    "export",
    "package",
    "formula"
  ],
  "author": "MomoCow",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/momocow/node-excel-module/issues"
  },
  "homepage": "https://github.com/momocow/node-excel-module#readme",
  "devDependencies": {
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/github": "^5.2.5",
    "@types/debug": "0.0.31",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.10",
    "@types/serialize-javascript": "^1.5.0",
    "ava": "^1.0.0-rc.2",
    "gitmoji-cli": "^1.9.2",
    "semantic-release": "^15.12.4",
    "semantic-release-gitmoji": "^1.2.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "base26": "github:jameslaneconkling/base26",
    "debug": "^4.1.0",
    "exceljs": "^1.6.3",
    "hot-formula-parser": "^3.0.0",
    "lodash": "^4.17.11",
    "serialize-javascript": "^1.5.0"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  }
}
